<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd" >
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>

	<typeAliases>
		<typeAlias alias="Employee" type="com.kh.ePERA.employment.employee.vo.Employee"/>
		<typeAlias alias="Room" type="com.kh.ePERA.room.model.vo.Room"/>
		<typeAlias alias="RSV" type="com.kh.ePERA.reservation.model.vo.Reservation"/>
		<typeAlias alias="IH" type="com.kh.ePERA.guest.inHouse.vo.InHouse"/>
		<typeAlias alias="IHR" type="com.kh.ePERA.guest.inHouseRequest.vo.InHouseRequest"/>
		<typeAlias alias="Shift" type="com.kh.ePERA.employment.shift.vo.Shift"/>
		<typeAlias alias="Att" type="com.kh.ePERA.employment.tna.vo.Attendance"/>
	</typeAliases>

	<mappers>
		<mapper resource="mappers/Employee-mapper.xml"/>
		<mapper resource="mappers/Room-mapper.xml"/>
		<mapper resource="mappers/Reservation-mapper.xml"/>
		<mapper resource="mappers/InHouse-mapper.xml"/>
		<mapper resource="mappers/InHouseRequest-mapper.xml"/>
		<mapper resource="mappers/Shift-mapper.xml"/>
		<mapper resource="mappers/Attendance-mapper.xml"/>
	</mappers>

</configuration>
